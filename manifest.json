{
    "name": "DataV Helper Extensions",
    "description": "DataV Helper Extensions",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "./js/background.js"
    },
    "content_scripts": [
        {
            "run_at": "document_idle",
            "matches": ["*://datav.aliyun.com/screen/*"],
            "css": ["content.css"],
            "js": ["./third-party/jquery-1.12.4.js", "./js/content.js"]
        }
    ],
    "permissions": ["tabs", "webRequest"],
    "host_permissions": ["*://datav.aliyun.com/*"]
}
